{
  "name": "react-webpack-boilerplate",
  "version": "2.0.0",
  "description": "Minimalistic ES6 React boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --devtool source-map --hot --inline --content-base public --history-api-fallback",
    "production": "cross-env NODE_ENV=production node_modules/.bin/webpack -p --config webpack.production.config.js",
    "bundle": "node_modules/.bin/webpack -p",
    "lint": "eslint app",
    "test": "mocha --compilers js:babel-register --recursive ./tests/**/*.js",
    "precommit": "npm run lint && npm test",
    "prepush": "npm run lint && npm test"
  },
  "author": "Kliment Petrov <kleopetroff@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "cross-env": "^3.0.0",
    "react": "^15.3.0",
    "react-dom": "^15.2.0",
    "react-router": "^2.5.2"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.10.4",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-boilerplate": "^1.1.1",
    "babel-preset-react-hmre": "^1.1.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.25.0",
    "enzyme": "^2.3.0",
    "eslint": "^3.0.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.0.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "husky": "^0.11.4",
    "mocha": "^3.0.0",
    "node-sass": "^3.8.0",
    "open-browser-webpack-plugin": "0.0.2",
    "react-hot-loader": "^3.0.0-beta.2",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.13",
    "webpack-dev-server": "^v2.1.0-beta.0"
  }
}
