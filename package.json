{
  "name": "react-webpack-boilerplate",
  "version": "2.1.0",
  "description": "Minimalistic ES6 React boilerplate",
  "main": "index.js",
  "engines": {
    "node": "8.11.1"
  },
  "scripts": {
    "start": "node server.js",
    "start:dist": "npm run production && node server.js",
    "dev": "webpack-dev-server --config webpack.dev.config.js",
    "clean": "rimraf dist",
    "production": "cross-env npm run clean && webpack --config webpack.production.config.js --progress --profile --colors",
    "lint": "eslint ./app/**/**.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "webpack:dev": "webpack --colors",
    "webpack:prod": "webpack --config webpack.production.config.js --colors",
    "heroku-postbuild": "npm run production"
  },
  "author": "Kliment Petrov <kleopetroff@gmail.com>",
  "license": "MIT",
  "jest": {
    "setupFiles": [
      "<rootDir>/config/jest/shim.js"
    ],
    "collectCoverageFrom": [
      "app/**/*.{js,jsx,ts,tsx}"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/config/jest/assetsTransformer.js"
    },
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ],
    "transform": {
      "\\.js$": "babel-jest"
    }
  },
  "dependencies": {
    "@material-ui/core": "^3.2.0",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.18.0",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.6",
    "cross-env": "5.2.0",
    "echarts": "^4.2.0-rc.2",
    "echarts-for-react": "^2.0.15-beta.0",
    "eventemitter3": "^3.1.0",
    "express": "^4.16.4",
    "firebase": "^5.7.0",
    "prop-types": "^15.6.2",
    "react": "16.5.2",
    "react-color": "^2.14.1",
    "react-dom": "16.5.2",
    "react-hot-loader": "4.3.11",
    "react-router-dom": "4.3.0",
    "react-sortable-tree": "^2.6.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "7.1.1",
    "@babel/core": "^7.1.1",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-stage-2": "7.0.0",
    "@babel/register": "7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "8.0.3",
    "babel-preset-react-hmre": "1.1.1",
    "copy-webpack-plugin": "4.5.2",
    "css-hot-loader": "1.4.2",
    "css-loader": "1.0.0",
    "enzyme": "3.6.0",
    "enzyme-adapter-react-16": "1.5.0",
    "eslint": "5.0.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.1",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.1.1",
    "jest": "23.6.0",
    "node-sass": "4.9.3",
    "open-browser-webpack-plugin": "0.0.5",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "16.5.2",
    "regenerator-runtime": "0.12.1",
    "rimraf": "2.6.2",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.0",
    "uglifyjs-webpack-plugin": "1.3.0",
    "url-loader": "1.0.1",
    "webpack": "4.19.1",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.9"
  }
}
